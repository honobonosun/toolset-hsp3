  "i18n-ally.localesPaths": ["./", "./l10n"],
  "i18n-ally.pathMatcher": "package.nls.{locale}.json",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.enabledFrameworks": ["vscode"]
}
