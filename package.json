{
  "name": "toolset-hsp3",
  "publisher": "honobonosun",
  "version": "0.7.1",
  "preview": true,
  "description": "%extension.description%",
  "main": "out/extension.js",
  "readme": "./readme.md",
  "extensionKind": [
    "workspace"
  ],
  "activationEvents": [
    "onLanguage:hsp3"
  ],
  "repository": "git@github.com:honobonosun/toolset-hsp3.git",
  "author": "Honobono",
  "license": "MIT",
  "private": true,
  "engines": {
    "vscode": "^1.70.3"
  },
  "l10n": "./l10n",
  "displayName": "toolset-hsp3",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "HSP3",
    "hsp",
    "auto-configuration"
  ],
  "contributes": {
    "configuration": {
      "title": "toolset-hsp3",
      "properties": {
        "toolset-hsp3.globs": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "/hsp3*"
          ],
          "scope": "machine",
          "markdownDescription": "%package.description.cfg.globs%"
        },
        "toolset-hsp3.override.enable": {
          "type": "boolean",
          "markdownDescription": "%package.description.cfg.override.enable%",
          "scope": "machine"
        },
        "toolset-hsp3.override.scope": {
          "type": "string",
          "default": "true",
          "markdownDescription": "%package.description.cfg.override.scope%",
          "enum": [
            "undefined",
            "false",
            "true",
            "Global",
            "Workspace",
            "WorkspaceFolder"
          ],
          "markdownEnumDescriptions": [
            "undefined",
            "false",
            "true",
            "%package.description.cfg.override.scope.Global%",
            "%package.description.cfg.override.scope.Workspace%",
            "%package.description.cfg.override.scope.WorkspaceFolder%"
          ]
        },
        "toolset-hsp3.override.applyChangesImmediately": {
          "type": "boolean",
          "scope": "machine",
          "markdownDescription": "%package.description.cfg.override.applyChangesImmediately%"
        },
        "toolset-hsp3.override.applyChangesImmediatelyInReloadWindow": {
          "type": "boolean",
          "scope": "machine",
          "markdownDescription": "%package.description.cfg.override.applyChangesImmediatelyInReloadWindow%"
        },
        "toolset-hsp3.override.list": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "scope": "machine",
          "examples": [
            [
              "vain0x.hsp3-debug-window-adapter.hsp3-root"
            ],
            [
              "vain0x.hsp3-debug-window-adapter.hsp3-root",
              "vain0x.hsp3-analyzer-mini.hsp3-root"
            ]
          ],
          "markdownDescription": "%package.description.cfg.override.list%"
        },
        "toolset-hsp3.override.listEx": {
          "type": "array",
          "items": {
            "type": "object"
          },
          "examples": [
            [
              {
                "publisher": "honobonosun",
                "id": "language-hsp3.helpman.path.local",
                "value": [
                  "%HSP3_ROOT%",
                  "hsphelp",
                  "helpman.exe"
                ],
                "platform": "win32",
                "scope": "WorkspaceFolder"
              }
            ]
          ],
          "markdownDescription": "%package.description.cfg.override.listEx%"
        },
        "toolset-hsp3.override.ignores": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "honobonosun\\.language-hsp3\\.helpman\\.path\\.local"
          ],
          "markdownDescription": "%package.description.cfg.override.ignores%",
          "scope": "machine"
        },
        "toolset-hsp3.task.env.enable": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "%package.description.cfg.task.env.enable%",
          "scope": "machine"
        },
        "toolset-hsp3.test.path": {
          "type": "string",
          "scope": "machine"
        }
      }
    },
    "configurationDefaults": {},
    "commands": [
      {
        "command": "toolset-hsp3.select",
        "title": "%package.title.cmd.select%",
        "category": "hsp.toolset"
      },
      {
        "command": "toolset-hsp3.unset",
        "title": "%package.title.cmd.unset%",
        "category": "hsp.toolset"
      },
      {
        "command": "toolset-hsp3.open",
        "title": "%package.title.cmd.open%",
        "category": "hsp.toolset"
      },
      {
        "command": "toolset-hsp3.override",
        "title": "%package.title.cmd.override%",
        "category": "hsp.toolset"
      },
      {
        "title": "insider:toolset-hsp3.current",
        "command": "toolset-hsp3.current",
        "enablement": "false"
      },
      {
        "title": "insider:toolset-hsp3.current.toString",
        "command": "toolset-hsp3.current.toString",
        "enablement": "false"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run -S esbuild-base -- --minify",
    "esbuild-base": "rimraf out && esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "build": "npm run -S esbuild-base -- --sourcemap",
    "watch": "npm run -S esbuild-base -- --sourcemap --watch",
    "fmt": "prettier --write \"src/**/*.ts\"&& npm run test -- --fix",
    "test": "eslint src --ext ts && tsc --noEmit",
    "genL10nFiles": "npx @vscode/l10n-dev export --outDir ./l10n ./src",
    "make": "npx vsce package"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.6",
    "@types/glob": "^8.0.0",
    "@types/i18n": "^0.13.8",
    "@types/is-glob": "^4.0.2",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^18.15.0",
    "@types/semver": "^7.5.0",
    "@types/vscode": "^1.70.3",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vscode/l10n": "^0.0.16",
    "@vscode/l10n-dev": "^0.0.29",
    "@vscode/vsce": "^2.21.0",
    "esbuild": "^0.15.10",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^8.10.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "glob": "^8.0.3",
    "i18n": "^0.15.1",
    "i18next": "^23.6.0",
    "is-glob": "^4.0.3",
    "micromatch": "^4.0.5",
    "semver": "^7.5.4",
    "zod": "^3.21.4"
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "toolset-hsp3": {
    "version": "1.0.0",
    "enable": true,
    "settings": [
      {
        "id": "toolset-hsp3.test.path",
        "value": [
          "%HSP3_ROOT%",
          "test"
        ],
        "scope": "Global"
      }
    ],
    "reloadWindow": false
  }
}
